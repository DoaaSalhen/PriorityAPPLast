@model IEnumerable<PriorityApp.Service.Models.MasterModels.ItemModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/NiceAdmin/_CSLayout.cshtml";

}
    <h1>Index</h1>
    <p>
        <a  asp-action="Create">Create New</a>
    </p>
    <table class="table table-hover">
        <thead>
            <tr>
                @*<th>
            @Html.DisplayNameFor(model => model.QutaCalc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HMP)
        </th>*@
                <th>
                    @Html.DisplayNameFor(model => model.Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.type)
                </th>
                <th>
                    Active
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CreatedDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UpdatedDate)
                </th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                @foreach (var item in Model)
                {
            <tr>
                @*<td>
            @Html.DisplayFor(modelItem => item.QutaCalc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HMP)
        </td>*@
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.type)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IsVisible)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CreatedDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UpdatedDate)
                </td>
                <td>
                    <a class="far fa-edit btn btn-success" asp-action="Edit" asp-route-id="@item.Id">Edit</a>
                    <a class="btn btn-info" asp-action="Details" asp-route-id="@item.Id">Details</a>
                    <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
                }
            }
        </tbody>
    </table>
